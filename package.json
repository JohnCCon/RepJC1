{
  "name": "repjc1",
  "version": "1.0.0",
  "description": "A comprehensive project with well-documented APIs, functions, and components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "docs:generate": "typedoc src --out docs/generated",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "api",
    "documentation",
    "ui-library"
  ],
  "author": "RepJC1 Team",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "axios": "^1.3.0",
    "date-fns": "^2.29.3",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "yup": "^1.0.0",
    "formik": "^2.2.9",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/lodash": "^4.14.191",
    "@types/uuid": "^9.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@storybook/react": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-viewport": "^6.5.16",
    "eslint": "^8.34.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "typescript": "^4.9.5",
    "typedoc": "^0.23.25",
    "react-scripts": "5.0.1",
    "sass": "^1.58.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/RepJC1.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/RepJC1/issues"
  },
  "homepage": "https://github.com/your-username/RepJC1#readme"
}